{
	"name": "abdallah-personal-website",
	"type": "module",
	"version": "1.0.0",
	"description": "Abdallah's personal website and blog built with Astro, Tailwind CSS, and TypeScript",
	"author": "AbdallahAHO",
	"license": "MIT",
	"private": true,
	"packageManager": "pnpm@8.15.4",
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"dev:network": "astro dev --host",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"preview:network": "astro preview --host",
		"astro": "astro",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --apply .",
		"check:all": "biome check --apply .",
		"check:unsafe": "biome check --apply-unsafe .",
		"clean": "rm -rf node_modules dist .astro",
		"reinstall": "pnpm clean && pnpm install",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.2.4",
		"@astrojs/node": "^9.2.0",
		"@astrojs/react": "^4.2.4",
		"@astrojs/rss": "^4.0.11",
		"@astrojs/sitemap": "^3.3.0",
		"@astrojs/tailwind": "^6.0.2",
		"@astrojs/vercel": "^8.1.3",
		"@fontsource/inter": "^5.0.17",
		"@fontsource/lora": "^5.0.16",
		"@tailwindcss/typography": "^0.5.15",
		"astro": "^5.7.2",
		"astro-compress": "^2.3.3",
		"astro-robots-txt": "^1.0.0",
		"clsx": "^2.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sharp": "^0.33.2",
		"tailwind-merge": "^2.2.1",
		"tailwindcss": "^3.4.1",
		"use-last-fm": "^0.6.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.35",
		"typescript": "^5.3.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AbdallahAHO/personal-website-2024.git"
	},
	"bugs": {
		"url": "https://github.com/AbdallahAHO/personal-website-2024/issues"
	},
	"homepage": "https://abdallahaho.com",
	"keywords": [
		"astro",
		"tailwindcss",
		"typescript",
		"personal-website",
		"blog"
	]
}
